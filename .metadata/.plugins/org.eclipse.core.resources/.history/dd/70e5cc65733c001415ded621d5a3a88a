package balls;

import java.awt.Color;
import java.awt.Component;

public class CurveBall extends ABall {


  public CurveBall (Color color, int xLoc, int yLoc, int xVel, int yVel, int radius, Component inCanvas)  {
    super (color, xLoc, yLoc, xVel, yVel, radius, inCanvas) ;
}
  @Override
  public void updateState() {
    // TODO Auto-generated method stub
    this.setXVel(getXVel()+1);
    this.setYVel(getYVel()+1);
  }

}
