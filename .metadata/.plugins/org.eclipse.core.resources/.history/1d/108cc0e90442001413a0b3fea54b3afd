import java.awt.Desktop;
import java.io.File;

public class OpenPageInDefaultBrowser {
  public static void main(String[] args) {
    try {
      /*
       * //Set your page url in this string. For eg, I m using URL for Google Search engine String
       * url = "http://water.clear.rice.edu:18024/index.html";
       * java.awt.Desktop.getDesktop().browse(java.net.URI.create(url));
       */

      String htmlFilePath = "http://water.clear.rice.edu:18024/index.html"; // path to your new file
      File htmlFile = new File(htmlFilePath);

      // open the default web browser for the HTML page
      // Desktop.getDesktop().browse(htmlFile.toURI());

      // if a web browser is the default HTML handler, this might work too
      Desktop.getDesktop().open(htmlFile);
    } catch (java.io.IOException e) {
      System.out.println(e.getMessage());
    }
  }
}
