package yn4_jjc7.client.model.message.chat;

import common.message.chat.ITextMessage;

/**
 * Concrete wrapper for an <code>ITextMessage</code>.
 */
public class TextMessage implements ITextMessage {

	/** Serial ID */
	private static final long serialVersionUID = 2471798252842661688L;

	/** The text of the message */
	private String text;

	/**
	 * Creates a text message with the specified text message.
	 * 
	 * @param text the message
	 */
	public TextMessage(String text) {
		this.text = text;
	}

	@Override
	public String getText() {
		return this.text;
	}

}
